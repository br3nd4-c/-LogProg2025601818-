                <then>
                    <if expression="donas&gt;=50">
                        <then>
                            <output expression="(precio*donas)-donas*2" newline="True"/>
                        </then>
                        <else>
                            <output expression="(precio*donas)-donas*1" newline="True"/>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="donas*precio" newline="True"/>
                </else>
            </if>
