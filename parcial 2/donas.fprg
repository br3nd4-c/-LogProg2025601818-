    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="precio" type="Integer" array="False" size=""/>
            <assign variable="precio" expression="10"/>
            <declare name="donas" type="Integer" array="False" size=""/>
            <assign variable="donas" expression="0"/>
